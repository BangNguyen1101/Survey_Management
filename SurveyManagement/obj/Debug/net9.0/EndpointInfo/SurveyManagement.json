    "schemas": {
      "Answer": {
        "type": "object",
        "properties": {
          "answerId": {
            "type": "integer",
            "format": "int32"
          },
          "questionId": {
            "type": "integer",
            "format": "int32"
          },
          "content": {
            "type": "string",
            "nullable": true
          },
          "isCorrect": {
            "type": "boolean",
            "nullable": true
          },
          "question": {
            "$ref": "#/components/schemas/Question"
          }
        },
        "additionalProperties": false
      },
      "AnswerDto": {
        "type": "object",
        "properties": {
          "content": {
            "type": "string",
            "nullable": true
          },
          "isCorrect": {
            "type": "boolean",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Badge": {
